{
  "NewsApi":{
    "BaseUrl": "https://hacker-news.firebaseio.com",
    "Version": "0"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "json",
      "FormatterOptions": {
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }

      }
    }
  },
  "AllowedHosts": "*"
}
